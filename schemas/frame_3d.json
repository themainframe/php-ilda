{
	"ilda_header": {
		"type": "ascii",
		"bytes": 4
	},

	"format_code": {
		"type": "unsigned-int",
		"bytes": 4
	},

	"frame_name": {
		"type": "ascii",
		"bytes": 8
	},

	"company_name": {
		"type": "ascii",
		"bytes": 8
	},

	"total_points": {
		"type": "unsigned-int",
		"bytes": 2
	},

	"frame_number": {
		"type": "unsigned-int",
		"bytes": 2
	},

	"total_frames": {
		"type": "unsigned-int",
		"bytes": 2
	},

	"scanner_head": {
		"type": "unsigned-int",
		"bytes": 1
	},

	"future": {
		"type": "unsigned-int",
		"bytes": 1
	},

	"points": {

		"type": "fieldset",
		"count": "@total_points",
		"fields": {

			"x_coordinate": {
				"type": "signed-int",
				"bytes": 2
			},

			"y_coordinate": {
				"type": "signed-int",
				"bytes": 2
			},

			"z_coordinate": {
				"type": "signed-int",
				"bytes": 2
			},

			"status_code": {
				"type": "compound",
				"bytes": 2,
				"fields": {

					"colour_number": {
						"type": "unsigned-int",
						"bits": 8
					},

					"unassigned": {
						"type": "ignored",
						"bits": 6
					},

					"blanking": {
						"type": "unsigned-int",
						"bits": 1
					},

					"last_point": {
						"type": "unsigned-int",
						"bits": 1
					}
					
				}
			},
		}
	}
}